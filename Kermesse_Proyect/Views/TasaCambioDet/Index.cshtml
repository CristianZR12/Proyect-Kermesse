@model IEnumerable<Kermesse_Proyect.Models.TasaCambioDet>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-11 mx-auto">
    <h2>Tasa de Cambio Detalle</h2>

    <p>
        @Html.ActionLink("Nuevo", "Create")
    </p>
    <table class="table table-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tipoCambio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TasaCambio1.mes)
            </th>
            <th>Actions</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.fecha)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.tipoCambio)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TasaCambio1.mes)
                </td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = item.idTasaCambioDet }) |
                    @Html.ActionLink("Detalle", "Details", new { id = item.idTasaCambioDet }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.idTasaCambioDet })
                </td>
            </tr>
        }

    </table>

</div>